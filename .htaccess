<IfModule mod_expires.c>
    ExpiresActive On
    ExpiresByType application/json "access plus 1 second"
    ExpiresByType text/html "access plus 1 second"
    ExpiresByType image/x-icon "access plus 13 month"
    ExpiresByType image/webp "access plus 13 month"
    ExpiresByType image/gif "access plus 13 month"
    ExpiresByType image/jpg "access plus 13 month"
    ExpiresByType image/jpeg "access plus 13 month"
    ExpiresByType text/css "access plus 13 month"
    ExpiresByType application/javascript "access plus 13 month"
    ExpiresByType text/javascript "access plus 13 month"
    ExpiresByType font/woff2 "access plus 13 month"
    ExpiresByType font/ttf "access plus 13 month"
    ExpiresByType font/otf "access plus 13 month"
</IfModule>
<IfModule mod_headers.c>
    Header unset Server
    Header set X-Powered-By Love
    Header unset X-CF-Powered-By
    Header unset X-Mod-Pagespeed
    Header unset X-Pingback
</IfModule>
